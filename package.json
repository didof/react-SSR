{
    "name": "react-ssr",
    "version": "1.0.0",
    "description": "Server side rendering project",
    "main": "index.js",
    "scripts": {
        "dev": "run-p dev:server dev:api dev:client:build",
        "dev:client:build": "webpack --config webpack/webpack.client.js",
        "dev:server:build": "webpack --config webpack/webpack.server.js",
        "dev:server:start": "nodemon --watch build --watch public --exec \"node build/bundle.server.js\"",
        "dev:server": "run-p dev:server:*",
        "dev:api:build": "webpack --config webpack/webpack.api.js",
        "dev:api:start": "nodemon --watch build --exec \"node build/bundle.api.js\"",
        "dev:api": "run-p dev:api:*"
    },
    "author": "didof",
    "license": "ISC",
    "dependencies": {
        "@reduxjs/toolkit": "^1.6.1",
        "axios": "0.16.2",
        "babel-cli": "6.26.0",
        "babel-core": "6.26.0",
        "babel-loader": "7.1.2",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "1.6.0",
        "babel-preset-es2015": "6.24.1",
        "babel-preset-es2017": "6.24.1",
        "babel-preset-react": "6.24.1",
        "babel-preset-stage-0": "6.24.1",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.19.0",
        "compression": "1.7.0",
        "concurrently": "3.5.0",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.15.4",
        "express-http-proxy": "^1.6.2",
        "jsonwebtoken": "^8.5.1",
        "lodash": "4.17.4",
        "nodemon": "1.12.0",
        "npm-run-all": "4.1.1",
        "opener": "^1.5.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-helmet": "^6.1.0",
        "react-redux": "^7.2.4",
        "react-router-config": "^5.1.1",
        "react-router-dom": "^5.2.0",
        "redux": "^4.1.0",
        "redux-devtools-extension": "^2.13.9",
        "redux-thunk": "^2.3.0",
        "serialize-javascript": "1.4.0",
        "webpack": "^3.5.6",
        "webpack-dev-server": "2.8.2",
        "webpack-merge": "4.1.0",
        "webpack-node-externals": "1.6.0"
    }
}
